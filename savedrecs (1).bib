@inproceedings{ WOS:000334270100022,
Author = {Mascret, Bruno and Mille, Alain and Guillet, Vivien},
Editor = {Miesenberger, K and Karshmer, A and Penaz, P and Zagler, W},
Title = {Supporting Braille Learning and Uses by Adapting Transcription to User's
   Needs},
Booktitle = {COMPUTERS HELPING PEOPLE WITH SPECIAL NEEDS, PT I},
Series = {Lecture Notes in Computer Science},
Year = {2012},
Volume = {7382},
Pages = {150-157},
Note = {13th International Conference on Computers Helping People with Special
   Needs (ICCHP), Linz, AUSTRIA, JUL 11-13, 2012},
Organization = {United Nat Educ, Sci \& Cultural Org; European Disabil Forum; Johannes
   Kepler Univ Linz},
Abstract = {This paper focuses on how to improve accessibility for Braille readers
   on Internet. We criticize actual technologies and show their limits in
   scientific Braille and Braille personnalization, especially in
   pedagogical situations. We present NAT Braille, a free software solution
   designed to respond to pedagogical specific needs. The transcribing
   process uses a set of customizable XSLT transformations and several XML
   formats. We detail the design of NAT Braille and the technologies used
   for transcriptions. Then we explain why NAT Braille improves
   personnalization in Braille rendering on Internet. We give the example
   of our Mozilla extension which is able to transcribe web pages including
   MathML markup, and is set up with adapted transcription rules taking
   into account the user's preferences. We conclude by raising issues
   related to our work.},
Publisher = {SPRINGER-VERLAG BERLIN},
Address = {HEIDELBERGER PLATZ 3, D-14197 BERLIN, GERMANY},
Type = {Proceedings Paper},
